{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2022",
    "baseUrl": "./",
    "paths": {
      "@/*": ["server/*"],
      "@types/*": ["server/types/*"],
      "@shared/*": ["server/shared/*"],
      "@config/*": ["server/config/*"],
      "@api/*": ["server/api/*"],
      "@middlewares/*": ["server/middlewares/*"]

    }
  },
  "module": {
    "type": "nodenext"
  }
}