{
  "version": 2,
  "builds": [
    {
      "src": "server/api/vercel.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/api/vercel.ts"
    }
  ],
    "buildCommand": "npm install && npx prisma generate  --schema=./prisma/schema.prisma && npm run build",
      "env": {
    "PRISMA_SCHEMA_PATH": "./prisma/schema.prisma"
  }

}
