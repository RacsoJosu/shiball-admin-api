{
  "version": 2,
  "builds": [
    {
      "src": "server/api/vercel.ts",
      "use": "@vercel/node"
    }
  ],
 
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://shinball-fronted.vercel.app" },
        { "key": "Access-Control-Allow-Origin", "value": "https://shinball-fronted-g1hd271xg-oscarvallecillos-projects.vercel.app" },
         { "key": "Access-Control-Allow-Origin", "value": "http://localhost:5173" },

        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ],
  "env": {
    "PRISMA_SCHEMA_PATH": "./prisma/schema.prisma"
  }
}
